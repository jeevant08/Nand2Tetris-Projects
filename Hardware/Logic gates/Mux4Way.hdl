/**
 * 4-way 16-bit multiplexor:
 * out = a if sel = 00
 *       b if sel = 01
 *       c if sel = 10
 *       d if sel = 11
 */
CHIP Mux4Way {
    IN a, b, c, d, sel[2];
    OUT out;
    
    PARTS:
	Mux(a=a, b=b, sel=sel[0], out=mx);
	Mux(a=c, b=d, sel=sel[0], out=m);
	Mux(a=mx, b=m, sel=sel[1], out=out);
	
}