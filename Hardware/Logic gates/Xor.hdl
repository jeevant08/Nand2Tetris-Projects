/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=NotA);
	Not(in=b, out=NotB);
	And(a=a, b=NotB, out=add1);
	And(a=b, b=NotA, out=add2);
	Or(a=add1, b=add2, out=out);
	
}